{
  "name": "vue3-ts-app-starter",
  "version": "1.0.1",
  "description": "The application template for Vue.js 3.x with TypeScript",
  "dependencies": {
    "core-js": "^3.23.1",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/mini-css-extract-plugin": "^2.5.0",
    "@types/node": "^16.11.41",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vue/compiler-sfc": "^3.2.37",
    "babel-loader": "^8.2.5",
    "babel-preset-typescript-vue3": "^2.0.17",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.18.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-scss": "^4.2.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "vue-loader": "^17.0.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.ts",
    "build:dev": "webpack --config webpack.dev.ts --watch",
    "start": "webpack serve --open --config webpack.dev.ts",
    "lint:eslint": "eslint src/**/*.{js,ts,vue}",
    "lint:stylelint": "stylelint src/**/*.{css,scss,vue}",
    "lint:prettier": "prettier src/**/*.{js,ts,css,scss,vue} --check",
    "lint": "run-p lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-s fix:*"
  },
  "private": true,
  "keywords": [],
  "author": "tmiyachi",
  "license": "MIT"
}
